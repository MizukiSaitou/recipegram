
<%= form_with model:@user, local:true do |f| %>

<%= f.label:username %>
<%= f.text_field:username %>

<%= f.label:email %>
<%= f.text_field:email %>

<%= f.label:profile %>
<%= f.text_area:profile %>

<%= f.label:profile_image %>
<%= f.attachment_field:profile_image %>

<%= f.submit"更新" %>

<% end %>